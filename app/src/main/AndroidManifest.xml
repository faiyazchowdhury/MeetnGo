<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.meetngo">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/letz_chill_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".status_page"
            android:screenOrientation="portrait" />
        <activity
            android:name=".select_groups"
            android:screenOrientation="portrait" />
        <activity
            android:name=".delete_members"
            android:screenOrientation="portrait" />
        <activity
            android:name=".create_group"
            android:screenOrientation="portrait" />
        <activity
            android:name=".contacts_in_phone"
            android:screenOrientation="portrait" />
        <activity
            android:name=".contacts_in_group"
            android:screenOrientation="portrait" />
        <activity
            android:name=".settings"
            android:screenOrientation="portrait" />
        <activity
            android:name=".groups"
            android:screenOrientation="portrait" />
        <activity
            android:name=".createaccount"
            android:screenOrientation="portrait" />
        <activity
            android:name=".forgotpassword"
            android:screenOrientation="portrait" />
        <activity
            android:name=".freeness"
            android:screenOrientation="portrait" />
        <activity
            android:name=".login"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".TimerandLocationService" />
        <service android:name=".QueryService" />
    </application>

</manifest>